<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE form-validation PUBLIC
    "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.1//EN"
    "http://jakarta.apache.org/commons/dtds/validator_1_1.dtd">

<form-validation>

    <formset>
		<form name="popupManageVO">
			<field property="popupTitleNm" depends="required, maxlength">
        		<arg0 key="팝업창명" resource="true"/>
        		<arg1 key="1024" resource="true"/>
				<var>
				<var-name>maxlength</var-name>
				<var-value>1024</var-value>
				</var>
			</field>

			<field property="fileUrl" depends="required, maxlength">
        		<arg0 key="팝업창URL" resource="true"/>
        		<arg1 key="1024" resource="true"/>
				<var>
				<var-name>maxlength</var-name>
				<var-value>1024</var-value>
				</var>
			</field>

			<field property="popupWlc" depends="required, maxlength, integer">
        		<arg0 key="팝업창위치-가로" resource="true"/>
        		<arg1 key="10" resource="true"/>
				<var>
				<var-name>maxlength</var-name>
				<var-value>10</var-value>
				</var>
			</field>

			<field property="popupHlc" depends="required, maxlength, integer">
        		<arg0 key="팝업창위치-세로" resource="true"/>
        		<arg1 key="10" resource="true"/>
				<var>
				<var-name>maxlength</var-name>
				<var-value>10</var-value>
				</var>
			</field>

			<field property="popupHSize" depends="required, maxlength, integer">
        		<arg0 key="팝업창사이즈-Height" resource="true"/>
        		<arg1 key="10" resource="true"/>
				<var>
				<var-name>maxlength</var-name>
				<var-value>10</var-value>
				</var>
			</field>

			<field property="popupWSize" depends="required, maxlength, integer">
        		<arg0 key="팝업창사이즈-WIDTH" resource="true"/>
        		<arg1 key="10" resource="true"/>
				<var>
				<var-name>maxlength</var-name>
				<var-value>10</var-value>
				</var>
			</field>

			<field property="stopVewAt" depends="required, maxlength">
        		<arg0 key="그만보기 설정 여부" resource="true"/>
        		<arg1 key="1" resource="true"/>
				<var>
				<var-name>maxlength</var-name>
				<var-value>1</var-value>
				</var>
			</field>

			<field property="ntceAt" depends="required, maxlength">
        		<arg0 key="게시 상태" resource="true"/>
        		<arg1 key="1" resource="true"/>
				<var>
				<var-name>maxlength</var-name>
				<var-value>1</var-value>
				</var>
			</field>
		</form>
    </formset>

</form-validation>